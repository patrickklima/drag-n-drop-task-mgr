{
  "name": "react-redux-dragndrop-task-mgr",
  "version": "1.0.0",
  "description": "A drag-n-drop task manager built as single page web app.",
  "engines": {
    "node": "8.9.4"
  },
  "main": "/server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "cd server && node server",
    "start": "cd server && node server",
    "postinstall": "npm run seeds && echo 'Server is ready. Start with npm run server'$'\n'",
    "client": "cd client && npm run start",
    "client:build": "cd client && npm run build",
    "client:install": "cd client && npm install && echo 'Client is ready. Start with npm run client'$'\n'",
    "seeds": "cd server/data/seeds && node index"
  },
